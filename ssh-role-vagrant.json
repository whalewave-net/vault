{
    "algorithm_signer" : "rsa-sha2-512",
    "allow_user_certificates": true,
    "allow_host_certificates": false,
    "allowed_users": "*",
    "allowed_extensions": "permit-pty,permit-port-forwarding",
    "default_extensions": [
    {
        "permit-pty": ""
    }
    ],
    "key_type": "ca",
    "default_user": "vagrant",
    "ttl": "30s"
}