{
    "backend": {
        "file": {
            "path":"/vault/file"
        }
    },
    "listener": {
        "tcp": {
            "address": "0.0.0.0:8200",
            "tls_disable": 0,
            "tls_cert_file": "/vault/keys/vault.whalewave.net.crt",
            "tls_key_file": "/vault/keys/vault.whalewave.net.key",
            "tls_disable_client_certs" : "true"
        }
    },
    "ui": true
}